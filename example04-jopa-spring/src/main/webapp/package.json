{
  "name": "jopa-example04",
  "version": "1.0.0",
  "description": "Example web application demonstrating JOPA in cooperation with Spring.",
  "main": "js/app.js",
  "dependencies": {
    "react": "~0.14.3",
    "react-dom": "~0.14.3",
    "react-bootstrap": "~0.27.3",
    "object-assign": "~4.0.1",
    "superagent": "~3.8.3",
    "reflux": "~0.3.0"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "envify": "^3.4.0",
    "reactify": "^0.15.2",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0",
    "babelify": "^7.2.0",
    "babel-core": "~6.26.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.3",
    "babel-preset-react": "^6.1.2",
    "webpack": "^1.12.11",
    "lodash": "^4.17.10"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "browserify": "browserify . -t [envify --NODE_ENV production] -o js/bundle.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.js",
    "prebuild": "npm install"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "Martin Ledvinka"
}
